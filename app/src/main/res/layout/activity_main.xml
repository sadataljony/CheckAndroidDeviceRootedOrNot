<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/btnCheckRootStatusUsingNativeCode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/check_device_root_using_native_code"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnCheckRootStatusUsingLibrary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/check_device_root_status_using_library"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCheckRootStatusUsingNativeCode" />

</androidx.constraintlayout.widget.ConstraintLayout>